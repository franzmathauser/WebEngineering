<header>
 <% logo = image_tag("logo.png", :alt => "Sample App", :class => "round") %>
<%= logo %>
  <nav class="round">
    <ul>
      <li><%= link_to "Home", '#' %></li>
      <% if signed_in? %>
        <li><%= link_to "Profile", current_user %></li>
      <% end %>
      <li><%= link_to "Help", '#' %></li>
      <% if signed_in? %>
        <li><%= link_to "Sign out", signout_path %></li>
      <% else %>
        <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
